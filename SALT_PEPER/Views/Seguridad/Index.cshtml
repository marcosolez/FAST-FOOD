@model List<ApplicationUser>

    <h1>Listado de usuarios</h1>

    <div class="card">
        <div class="card-body">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <td>Usuario</td>
                        <td>Email</td>
                    </tr>
                </thead>  
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>@item.UserName</td>
                        <td>@item.Email</td>
                       
                    </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>